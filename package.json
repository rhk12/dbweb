{
  "name": "react-responsive-navigation",
  "version": "1.0.0",
  "description": "A simple React.JS Responsive  Navigation with React Router and Styled Components.",
  "keywords": [
    "responsive  nav",
    "styled components",
    "react.js",
    "react router",
    "css3"
  ],
  "main": "src/index.js",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "font-awesome": "^4.7.0",
    "mdbreact": "^4.15.0",
    "aws-sdk": "^2.474.0",
    "react": "16.3.2",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-bootstrap-tabs": "^1.0.2",
    "react-dom": "16.3.2",
    "react-fontawesome": "^1.6.1",
    "react-google-charts": "^3.0.13",
    "react-native-linear-gradient": "^2.5.4",
    "react-page-scroller": "^1.5.1",
    "react-router-dom": "4.3.0",
    "react-scripts": "1.1.4",
    "styled-components": "3.3.2"
  },
  "devDependencies": {},
  "scripts": {
    "init": "concurrently \"npm run server-init\" \"npm run client-init\"",
    "server-init": "npm install",
    "client-init": "cd client &&  npm install",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run client\"",
    "server-dev": "nodemon server.js",
    "server": "node server.js",
    "client": "node start-client.js"
  }
}
